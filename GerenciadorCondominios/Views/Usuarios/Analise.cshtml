@model string
@{
    Layout = null;
    ViewData["Title"] = "Conta aprovação";
}

<!DOCTYPE html>

<html>
<head>
    <environment include="Development">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.css">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      

    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.mim.css">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
       

    </environment>
    <meta name="viewport" content="width=device-width"/>
    <title>Conta aprovação</title>
</head>
<body class="white-text blue darken-2 container" style="position: absolute; top: 40%; left: 20%">
<div>
    <h1>Conta em aprovação</h1>
    <h5>Olá @Model ! sua conta esta sob análise. Em breve voce poderá acessar o sistema.</h5>
</div>
</body>
</html>
